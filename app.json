{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.boltexponativewind",
      "infoPlist": {
        "NSUserNotificationUsageDescription": "This app uses notifications to remind you of your goals and provide encouragement messages.",
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby designated smoking areas on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Background location is used to monitor zones and dwell time to provide helpful notifications.",
        "NSMicrophoneUsageDescription": "The microphone is used to detect characteristic lighter or inhalation sounds to help prevent smoking (optional).",
        "UIBackgroundModes": [
          "location"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-notifications",
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.boltexponativewind",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "RECORD_AUDIO"
      ]
    },
